import graph


class Person(graph.Node):
    pass


class Device(graph.Token):
    pass


class Location(graph.Node):
    pass


class PartModel(graph.Resource):
    pass


class DeviceModel(graph.Resource):
    pass


class Batch(graph.Token):
    pass


class Route(graph.Path):
    pass


class Operation(Route):
    pass


class Part(Batch):
    def __init__(self, **kwargs):
        Batch(qty=1, **kwargs)

